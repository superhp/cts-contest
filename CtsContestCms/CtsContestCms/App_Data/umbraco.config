    <task id="1493" key="fcacc44a-54b1-41f0-98f0-a6c1efb20776" parentID="1153" level="2" creatorID="3" sortOrder="257" createDate="2017-10-10T07:10:10" updateDate="2017-10-10T07:12:26" nodeName="SETTLER'S TRAINING" urlName="settlers-training" path="-1,1153,1493" isDoc="" nodeType="1145" creatorName="zilvinas" writerName="zilvinas" writerID="3" template="1144" nodeTypeAlias="task">
      <input><![CDATA[3 3\n1 2 3
