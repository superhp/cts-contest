      <output><![CDATA[9991.27897663
4268.87997505
16617.2400536]]></output>
    </task>
    <task id="1453" key="de16746e-310e-4f2f-934d-e2c5a42e26bf" parentID="1153" level="2" creatorID="3" sortOrder="218" createDate="2017-10-04T11:22:49" updateDate="2017-10-04T11:26:21" nodeName="SPREADSHEET" urlName="spreadsheet" path="-1,1153,1453" isDoc="" nodeType="1145" creatorName="zilvinas" writerName="zilvinas" writerID="3" template="1144" nodeTypeAlias="task">
