    <task id="1523" key="fea234f9-d696-4e4a-a87f-6d106fa54336" parentID="1153" level="2" creatorID="3" sortOrder="275" createDate="2017-10-11T09:21:13" updateDate="2017-10-11T09:23:05" nodeName="PETYA AND JAVA" urlName="petya-and-java" path="-1,1153,1523" isDoc="" nodeType="1145" creatorName="zilvinas" writerName="zilvinas" writerID="3" template="1144" nodeTypeAlias="task">
