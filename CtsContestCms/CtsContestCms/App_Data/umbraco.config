    </task>
    <task id="1379" key="f1efb4c9-cbfd-457f-9279-f63dec5d90d6" parentID="1153" level="2" creatorID="3" sortOrder="145" createDate="2017-09-21T07:35:04" updateDate="2017-09-29T09:56:16" nodeName="REGULAR BRACKET SEQUENCE" urlName="regular-bracket-sequence" path="-1,1153,1379" isDoc="" nodeType="1145" creatorName="zilvinas" writerName="zilvinas" writerID="3" template="1144" nodeTypeAlias="task">
